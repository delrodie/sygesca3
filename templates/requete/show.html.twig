{% extends 'base.html.twig' %}

{% block title %}Requete{% endblock %}

{% block body %}
    <h1>Requete</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ requete.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ requete.nom }}</td>
            </tr>
            <tr>
                <th>Prenoms</th>
                <td>{{ requete.prenoms }}</td>
            </tr>
            <tr>
                <th>Datenaissance</th>
                <td>{{ requete.datenaissance }}</td>
            </tr>
            <tr>
                <th>Lieunaissance</th>
                <td>{{ requete.lieunaissance }}</td>
            </tr>
            <tr>
                <th>Contact</th>
                <td>{{ requete.contact }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ requete.message }}</td>
            </tr>
            <tr>
                <th>Media</th>
                <td>{{ requete.media }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ requete.statut }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ requete.createdAt ? requete.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('requete_index') }}">back to list</a>

    <a href="{{ path('requete_edit', {'id': requete.id}) }}">edit</a>

    {{ include('requete/_delete_form.html.twig') }}
