<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}ASSCI::{% endblock %}</title>
        <link rel="icon" href="{{ absolute_url(asset('assets/images/ascci.png') )}}">
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ absolute_url(asset('assets/fonts/material-design-iconic-font/css/material-design-iconic-font.min.css')) }}">
            <link rel="stylesheet" href="{{ absolute_url(asset('assets/css/style.css')) }}">
        {% endblock %}
    </head>
    <body style="background-image: url({{ absolute_url(asset('assets/images/bg-registration-form-2.jpg')) }}); background-size: cover;">
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ absolute_url(asset('assets/js/jquery.min.js') )}}"></script>
            <script charset="utf-8" src="https://www.cinetpay.com/cdn/seamless_sdk/latest/cinetpay.prod.min.js" type="text/javascript"></script>
            <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
            <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
            <!--<script src="{{ absolute_url(asset('assets/js/formulaire.js')) }}"></script>-->
        {% endblock %}
    </body>
</html>
